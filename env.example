# Example Environment Variables
# Copy this file to .env and update the values

# Application Settings
VITE_APP_ENV=development
VITE_APP_NAME=React Template Project
VITE_APP_VERSION=0.1.0

# API Configuration
VITE_API_BASE_URL=http://localhost:8080

# Feature Flags
VITE_ENABLE_LOGGING=true
VITE_LOG_LEVEL=debug
VITE_ENABLE_PERFORMANCE_MONITORING=false
VITE_ENABLE_API_MOCKING=true
VITE_ENABLE_DETAILED_ERRORS=true
VITE_ENABLE_DEBUG_TOOLS=true

# External Services
VITE_SENTRY_DSN=your-sentry-dsn-here
VITE_GOOGLE_ANALYTICS_ID=your-ga-id-here

# Security Settings
VITE_CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173
VITE_CSP_SETTINGS=default-src 'self'; script-src 'self'; img-src 'self' data:;
VITE_HSTS_SETTINGS=max-age=31536000; includeSubDomains; preload

# Performance Settings
VITE_HEALTH_CHECK_INTERVAL=60000
VITE_CACHE_TTL=0
VITE_RATE_LIMIT=100
